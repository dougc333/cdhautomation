# hbase experiment dougc
#when running htop I only see half the cpus pegged at 100%
#
#

[global]
thread=1
ioengine=/opt/dssd/lib64/fioflood.so.1
libflood_tune=vpci.qmax=4
volume=dougc
type=block
unlink=0
runtime=60
time_based=1
numjobs=1


[Test]
filename=dougctest
flen=32k
blocksize=128m
size=200G
iodepth=64
invalidate=0
group_reporting=1
direct=1
rw=randread
#norandommap
#rwmixread=100
#rwmixwrite=0
