# hbase experiment dougc
#when running htop I only see half the cpus pegged at 100%
#
#

[global]
thread=1
ioengine=/opt/dssd/lib64/fioflood.so.1
volume=dougc
type=block
unlink=0
runtime=120
time_based=1
numjobs=4


[Test]
filename=dougctest
flen=32k
blocksize=128m
size=1G
iodepth=64
invalidate=0
group_reporting=1
direct=1
readwrite=rw
#norandommap
rwmixread=100
rwmixwrite=0
